(function ($, Drupal) {

  'use strict';

  // Utility Menu
  Drupal.behaviors.facets = {
    attach: function (context) {

      const facetBlock = $('.facet-block');

      setTimeout( function() {
        facetBlock.each(function () {
          if ($(this).find('.facets-checkbox:checked').length) {
            $(this).addClass('is-active');
            $(this).find('.facet-block__content').slideDown(0);
            $(this).find('.facets-checkbox:checked').siblings('ul').slideDown(0);
          }

          if ($(this).find('.facet-item--active-trail').length) {
            $(this).find('.facet-item--active-trail').find('.facet-item__trigger-link').addClass('is-active');
          }
        });
      }, 500);
      
      
      // Facet block
      $('.facet-block__trigger', context).once().on('click', function (e) {
        e.preventDefault();
        $(this).parents('.facet-block').toggleClass('is-active');
        $(this).parent().siblings('.facet-block__content').slideToggle(300);
      });

      // Facet Group
      $('.facet-item__trigger-link', context).once().on('click', function (e) {
        e.preventDefault();
        $(this).toggleClass('is-active');
        $(this).parents('.facet-item--expanded').find('ul').slideToggle(300);
      });
      
    }
  }

})(jQuery, Drupal);;
(function ($, Drupal) {

  'use strict';

  // Utility Menu
  Drupal.behaviors.archive = {
    attach: function (context) {

      // Archive Tooltip
      const archiveFlag = $('.js-archive-tooltip');

      if (archiveFlag.length) {

        if ($('body').hasClass('language-en')) {
          var tooltip = $('<div class="archive-tooltip"><div class= "archive-tooltip__title"> Archived</div><div class="archive-tooltip__description">You are accessing a resource from the BioInteractive Archive. Archived resources are not updated to reflect current scientific knowledge, technology, and/or pedagogy.</div></div >');
        }
        else {
          var tooltip = $('<div class="archive-tooltip"><div class= "archive-tooltip__title"> Archivado</div><div class="archive-tooltip__description">Ests accediendo a un recurso archivado. Los recursos archivados pueden estar usando tecnologa desactualizada o incluir contenido cientfico o pedaggico que no ha sido actualizado.</div></div >');
        }

        archiveFlag.parents('.resource-header,.blog-header').append(tooltip);
      }

      archiveFlag.on('mouseover', function () {
        const archivePosition = archiveFlag.position();
        const archiveLeft = archivePosition.left;
        const archiveTop = archivePosition.top;
        archiveFlag.parents('.resource-header,.blog-header').find('.archive-tooltip').css({ top: archiveTop + 'px', left: archiveLeft + 'px' });
        $(this).parents('.resource-header,.blog-header').find('.archive-tooltip').addClass('is-active');
      });

      archiveFlag.on('mouseout', function () {
        $(this).parents('.resource-header,.blog-header').find('.archive-tooltip').removeClass('is-active');
      });

    }
  }

})(jQuery, Drupal);
;
(function ($, Drupal) {

  'use strict';

  // Utility Menu
  Drupal.behaviors.addthis = {
    attach: function (context) {

      const shareLink = $('.action-bar__link--share');

      shareLink.once().on('click', function (e) {
        e.preventDefault();
        const addthis = $(this).parents('.action-bar__share').find('.js-addthis-links');

        if (addthis.hasClass('is-active')) {
          addthis.removeClass('is-active');
        }
        else {
          addthis.addClass('is-active');

          setTimeout(function () {
            // If user clicks outside the account menu, close it
            $(document).one('click', function clickAway(e) {
              if (e.target.closest('.js-addthis-links')) {
                $(document).one('click', clickAway);
              } else {
                addthis.removeClass('is-active');
              }
            });

            // Close account menu if user presses escape key
            $(document).one('keyup', function (e) {
              if (e.key === "Escape") {
                addthis.removeClass('is-active');

                // Unbind the document.one('click') set above
                $(document).unbind('click');
              }
            });
          }, 200)
        }

      });

    }
  }

})(jQuery, Drupal);;
(function ($) {

  'use strict';

  // complex Teaser
  Drupal.behaviors.analytics = {
    attach: function (context) {
      if (typeof dataLayer !== 'undefined') {
        const userForm = $('#user-register-form');

        const flattenUserForm = function (array, oldArray) {
          const newUserObject = {};
          newUserObject['grade_level_taught'] = [];
          newUserObject['grade_level_interest'] = [];
          newUserObject['topics_taught'] = [];
          newUserObject['topics_interest'] = [];
          newUserObject['science_practices'] = [];

          $.map(array, function (n) {
            var value = oldArray[n.name];
            if (n.name.includes('field_user_grade_level_taught')) {
              newUserObject['grade_level_taught'].push(value.attributes['data-term-name'].value);
            }
            if (n.name.includes('field_user_grade_level_interest')) {
              newUserObject['grade_level_interest'].push(value.attributes['data-term-name'].value);
            } else if (n.name.includes('field_user_topics_taught')) {
              newUserObject['topics_taught'].push(value.attributes['data-term-name'].value);
            } else if (n.name.includes('field_user_topics_interest')) {
              newUserObject['topics_interest'].push(value.attributes['data-term-name'].value);
            } else if (n.name.includes('field_user_science_practices')) {
              newUserObject['science_practices'].push(value.attributes['data-term-name'].value);
            } else if (n.name.includes('field_user_location[0][address]')) {
              const trimName = n.name.replace("field_user_location[0][address]", "").replace("[", "").replace("]", "");
              newUserObject[trimName] = (n.value);
            } else {
              const trimName = n.name.replace("field_user_", "").replace("[0]", "").replace("[value]", "");
              newUserObject[trimName] = n.value;
            }
          });

          // Remove the password fields
          delete newUserObject["pass[pass1]"];
          delete newUserObject["pass[pass2]"];

          return newUserObject;
        }

        userForm.submit(function (event) {
          // event.preventDefault();
          const userObject = flattenUserForm($(this).serializeArray(), this);
          dataLayer.push({
            'userRegistration': userObject,
            'event': 'userRegistration'
          });
        });

        //console.log(dataLayer);

        // Check if user is logged in on all pages
        if ($('body').hasClass('user-logged-in')) {
          dataLayer.push({
            'authenticatedUser': true,
            'event': 'authenticatedUser'
          });
        } else {
          dataLayer.push({
            'authenticatedUser': false,
            'event': 'authenticatedUser'
          });
        }
      }

    }
  }

})(jQuery);
;
// Custom scripts file

(function ($) {

  'use strict';

  // Generic function that runs on window resize.
  function resizeStuff() {

  }

  // Runs function once on window resize.
  var TO = false;
  $(window).resize(function () {
    if (TO !== false) {
      clearTimeout(TO);
    }

    // 200 is time in miliseconds.
    TO = setTimeout(resizeStuff, 200);
  }).resize();


  // My Library Menu Accordions
  Drupal.behaviors.myLibraryMenu = {
    attach: function (context) {

      const parentLink = $('.menu--my-library .menu__link.has-children');

      parentLink.once().on('click', function (e) {
        e.preventDefault();

        if ($(this).hasClass('is-open')) {
          $(this).removeClass('is-open');
          $(this).siblings('.menu__view').slideUp(250);
        }
        else {
          $(this).addClass('is-open');
          $(this).siblings('.menu__view').slideDown(250);
        }
      });

    }
  }


  // Squish header on scroll
  $(window).scroll(function () {
    const scroll = $(window).scrollTop();
    if (scroll > 50) {
      $(".l-header").addClass("l-header--squished");
      $(".user-drawer").addClass("user-drawer--scroll");
    }
    else {
      $(".l-header").removeClass("l-header--squished");
      $(".user-drawer").removeClass("user-drawer--scroll");
    }

    // Mobile Header when Logged in
    if (scroll > 10) {
      $(".mobile-menu").addClass("mobile-menu--scrolled");
      $(".user-drawer").addClass("mobile-menu--scrolled");
    }
    else {
      $(".mobile-menu").removeClass("mobile-menu--scrolled");
      $(".user-drawer").removeClass("mobile-menu--scrolled");
    }
  });


  // Favorite Flag Class Add
  $('.favorite-flag').not('.favorite-flag--favorited-by').on('click', function (e) {
    e.preventDefault();
    $(this).toggleClass('is-favorited');
  });


  // Info Accordion JS
  $('.info-accordion__title').on('click', function (e) {
    e.preventDefault();
    $(this).parent().toggleClass('is-open');
    $(this).next().slideToggle(250);
  });


  // Playlist Counter
  const complexTeaser = $('.teaser--complex');

  complexTeaser.each(function (item) {
    var title = $(this).find('.teaser__title a');
    const titleText = title.html();
    title[0].innerHTML = (item + 1) + '. ' + titleText;
  });

  // Scroll Magic Explore Link Reveal
  var controller = new ScrollMagic.Controller();
  new ScrollMagic.Scene({ triggerElement: ".featured-slider" }).setClassToggle("#featured-slider__explore-link", "is-shown").addTo(controller);

  // Scroll Magic sticky save button
  if (document.body.classList.contains('node-page--node-type-personal-resource-playlist') ||
    document.body.classList.contains('body--add-personal-resource-playlist')) {
    new ScrollMagic.Scene({
      triggerElement: ".l-footer",
      reverse: true,
      offset: -48,
      triggerHook: 1
    })
      .setClassToggle(".form-actions", "is-unfixed").addTo(controller);
  }

  // Remote Video Iframe Title
  $('.remote-video').each( function() {
    const videoTitle = $(this).attr('data-video-title');
    $(this).find('iframe').attr('title', videoTitle);
  });


  // Show More / Show Less toggle on simple teasers
  Drupal.behaviors.teaserToggle = {
    attach: function () {
      const langcode = document.documentElement.lang;
      if (langcode === 'es') {
        var field = $('.expand-collapse');
        field.each(function () {
          this.setAttribute('data-link-text-open', 'Ver ms');
          this.setAttribute('data-link-text-close', 'Ver menos');
        });

        var toggleLink = $('.ecf-open');
        toggleLink.each(function () {
          this.innerHTML = 'Ver ms';
        });
      }
    }
  };

})(jQuery);
