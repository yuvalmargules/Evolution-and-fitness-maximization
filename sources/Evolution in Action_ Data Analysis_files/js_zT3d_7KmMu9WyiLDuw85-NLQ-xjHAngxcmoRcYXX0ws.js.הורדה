his.axis[e].track.rect[this.axis[e].sizeAttr]; if (this.axis[e].isOverflowing) { var n = r / i; return t = Math.max(~~(n * r), this.options.scrollbarMinSize), this.options.scrollbarMaxSize && (t = Math.min(t, this.options.scrollbarMaxSize)), t } } }, { key: "positionScrollbar", value: function () { var t = 0 < arguments.length && void 0 !== arguments[0] ? arguments[0] : "y", e = this.contentEl[this.axis[t].scrollSizeAttr], i = this.axis[t].track.rect[this.axis[t].sizeAttr], r = parseInt(this.elStyles[this.axis[t].sizeAttr], 10), n = this.axis[t].scrollbar, o = this.contentEl[this.axis[t].scrollOffsetAttr], s = (o = "x" === t && this.isRtl && l.getRtlHelpers().isRtlScrollingInverted ? -o : o) / (e - r), a = ~~((i - n.size) * s); a = "x" === t && this.isRtl && l.getRtlHelpers().isRtlScrollbarInverted ? a + (i - n.size) : a, n.el.style.transform = "x" === t ? "translate3d(".concat(a, "px, 0, 0)") : "translate3d(0, ".concat(a, "px, 0)") } }, { key: "toggleTrackVisibility", value: function () { var t = 0 < arguments.length && void 0 !== arguments[0] ? arguments[0] : "y", e = this.axis[t].track.el, i = this.axis[t].scrollbar.el; this.axis[t].isOverflowing || this.axis[t].forceVisible ? (e.style.visibility = "visible", this.contentEl.style[this.axis[t].overflowAttr] = "scroll") : (e.style.visibility = "hidden", this.contentEl.style[this.axis[t].overflowAttr] = "hidden"), this.axis[t].isOverflowing ? i.style.visibility = "visible" : i.style.visibility = "hidden" } }, { key: "hideNativeScrollbar", value: function () { if (this.offsetEl.style[this.isRtl ? "left" : "right"] = this.axis.y.isOverflowing || this.axis.y.forceVisible ? "-".concat(this.scrollbarWidth || this.minScrollbarWidth, "px") : 0, this.offsetEl.style.bottom = this.axis.x.isOverflowing || this.axis.x.forceVisible ? "-".concat(this.scrollbarWidth || this.minScrollbarWidth, "px") : 0, !this.scrollbarWidth) { var t = [this.isRtl ? "paddingLeft" : "paddingRight"]; this.contentEl.style[t] = this.axis.y.isOverflowing || this.axis.y.forceVisible ? "calc(".concat(this.elStyles[t], " + ").concat(this.minScrollbarWidth, "px)") : this.elStyles[t], this.contentEl.style.paddingBottom = this.axis.x.isOverflowing || this.axis.x.forceVisible ? "calc(".concat(this.elStyles.paddingBottom, " + ").concat(this.minScrollbarWidth, "px)") : this.elStyles.paddingBottom } } }, { key: "onMouseMoveForAxis", value: function () { var t = 0 < arguments.length && void 0 !== arguments[0] ? arguments[0] : "y"; this.axis[t].track.rect = this.axis[t].track.el.getBoundingClientRect(), this.axis[t].scrollbar.rect = this.axis[t].scrollbar.el.getBoundingClientRect(), this.isWithinBounds(this.axis[t].scrollbar.rect) ? this.axis[t].scrollbar.el.classList.add(this.classNames.hover) : this.axis[t].scrollbar.el.classList.remove(this.classNames.hover), this.isWithinBounds(this.axis[t].track.rect) ? (this.showScrollbar(t), this.axis[t].track.el.classList.add(this.classNames.hover)) : this.axis[t].track.el.classList.remove(this.classNames.hover) } }, { key: "onMouseLeaveForAxis", value: function () { var t = 0 < arguments.length && void 0 !== arguments[0] ? arguments[0] : "y"; this.axis[t].track.el.classList.remove(this.classNames.hover), this.axis[t].scrollbar.el.classList.remove(this.classNames.hover) } }, { key: "showScrollbar", value: function () { var t = 0 < arguments.length && void 0 !== arguments[0] ? arguments[0] : "y", e = this.axis[t].scrollbar.el; this.axis[t].isVisible || (e.classList.add(this.classNames.visible), this.axis[t].isVisible = !0), this.options.autoHide && this.hideScrollbars() } }, { key: "onDragStart", value: function (t) { var e = 1 < arguments.length && void 0 !== arguments[1] ? arguments[1] : "y", i = this.axis[e].scrollbar.el, r = "y" === e ? t.pageY : t.pageX; this.axis[e].dragOffset = r - i.getBoundingClientRect()[this.axis[e].offsetAttr], this.draggedAxis = e, document.addEventListener("mousemove", this.drag), document.addEventListener("mouseup", this.onEndDrag) } }, { key: "getScrollElement", value: function () { return this.contentEl } }, { key: "removeListeners", value: function () { var e = this; this.options.autoHide && this.el.removeEventListener("mouseenter", this.onMouseEnter), ["mousedown", "click", "dblclick", "touchstart", "touchend", "touchmove"].forEach(function (t) { e.el.removeEventListener(t, e.onPointerEvent) }), this.el.removeEventListener("mousemove", this.onMouseMove), this.el.removeEventListener("mouseleave", this.onMouseLeave), this.contentEl.removeEventListener("scroll", this.onScroll), window.removeEventListener("resize", this.onWindowResize), this.mutationObserver && this.mutationObserver.disconnect(), this.resizeObserver.disconnect(), this.recalculate.cancel(), this.onMouseMove.cancel(), this.hideScrollbars.cancel(), this.onWindowResize.cancel() } }, { key: "unMount", value: function () { this.removeListeners(), this.el.SimpleBar = null } }, { key: "isChildNode", value: function (t) { return null !== t && (t === this.el || this.isChildNode(t.parentNode)) } }, { key: "isWithinBounds", value: function (t) { return this.mouseX >= t.left && this.mouseX <= t.left + t.width && this.mouseY >= t.top && this.mouseY <= t.top + t.height } }]) && ve(t.prototype, e), i && ve(t, i), l }(); return er.defaultOptions = { autoHide: !0, forceVisible: !1, classNames: { content: "simplebar-content", offset: "simplebar-offset", mask: "simplebar-mask", wrapper: "simplebar-wrapper", placeholder: "simplebar-placeholder", scrollbar: "simplebar-scrollbar", track: "simplebar-track", heightAutoObserverWrapperEl: "simplebar-height-auto-observer-wrapper", heightAutoObserverEl: "simplebar-height-auto-observer", visible: "simplebar-visible", horizontal: "simplebar-horizontal", vertical: "simplebar-vertical", hover: "simplebar-hover" }, scrollbarMinSize: 25, scrollbarMaxSize: 0, timeout: 1e3 }, tr && er.initHtmlApi(), er });;
/**
 * @file
 * classroom-resources.js
 *
 * JavaScript that rewrites the exposed form filters on the Classroom Resources 
 * landing pages and selects facet filters when form is submitted.
 */

(function ($, Drupal) {

  'use strict';

    Drupal.behaviors.classroomResources = {
      attach: function (context) {
        $('.landing-form #views-exposed-form-resources-default #edit-submit-resources').on('click', function (e) {
          const form = document.getElementById('views-exposed-form-resources-default');

          const level = $('#edit-level').val();
          if (level != 'All') {
            var input = document.createElement('input');
            input.type = 'hidden';
            input.name = 'f[0]';
            input.value = 'grade_levels:' + level;
            form.appendChild(input);
          }
          const resourceType = $('#edit-resource-type').val();
          if (resourceType != 'All') {
            var input = document.createElement('input');
            input.type = 'hidden';
            input.name = 'f[1]';
            input.value = 'resource_type:' + resourceType;
            form.appendChild(input);
          }
          const topics = $('#edit-topics').val();
          if (topics != 'All') {
            var input = document.createElement('input');
            input.type = 'hidden';
            input.name = 'f[2]';
            input.value = 'topics:' + topics;
            form.appendChild(input);
          }
        });
  
      }
    }
  }
)(jQuery, Drupal);;
(function ($, Drupal) {

  'use strict';

  // The styling for this mobile menu is located in pattern-lab/source/_patterns/03-components/mobile-menu/_mobile-menu.scss.

  Drupal.behaviors.mobileMenu = {
    attach: function (context) {

      // Create mobile menu container, create mobile bar, and clone the main
      // menu in the navigation region.
      var $mobileNav = $('<nav class="mobile-menu" role="navigation"></nav>'),
          $mobileBar = $('<div class="mobile-menu__bar"><button href="#" class="user-drawer-trigger js-user-drawer-trigger">User Menu</button><a href="/" class="site-name">BioInteractive Home</a><button class="mobile-menu__button js-mobile-menu-button mobile-menu__button--menu">Menu</button></div>'),
          $mobileLinks = $('<div class="mobile-menu__links hidden"></div>'),
          $languageSwitcher = $('.l-header', context).find('.language-switcher').first().clone();

      // Setup main menu variable based on langugage
      if ($('.l-header .menu--main').length) {
        var $mainMenu = $('.l-header', context).find('.menu--main').first().clone();
      }
      else if ($('.l-header .menu--main-es').length) {
        var $mainMenu = $('.l-header', context).find('.menu--main-es').first().clone();
      }

      // Setup utility menu variable based on langugage
      if ($('.l-header .menu--utility').length) {
        var $utilityMenu = $('.l-header', context).find('.menu--utility').first().clone();
      }
      else if ($('.l-header .menu--utility-es').length) {
        var $utilityMenu = $('.l-header', context).find('.menu--utility-es').first().clone();
      }

      // Only create mobile menu if there is a main menu.
      if (typeof $mainMenu !== 'undefined' && $mainMenu.length > 0) {

        // Remove menu id, add class, and format subnav menus.
        $mainMenu.removeAttr('id').attr('class', 'menu menu--mobile');

        // Remove children menu items.
        $mainMenu.find('ul').remove();

        // Insert the cloned menus into the mobile menu container.
        $mainMenu.appendTo($mobileLinks);
        $utilityMenu.appendTo($mobileLinks);
        $languageSwitcher.appendTo($mobileLinks);


        // insert search button and clone/append search bar, if it exists.
        if (!($('.mobile-menu .mobile-menu__search').length > 0)) {
          if ($('.block--search').length > 0) {
            $('.block--search').clone().addClass('mobile-menu__search').appendTo($mobileNav);
            $mobileBar.append('<button class="mobile-menu__button js-mobile-search-button mobile-menu__button--search"><span class="mobile-menu__icon mobile-menu__icon--search">Search</span></button>');
          }
        }

        // Insert the top bar into mobile menu container.
        $mobileBar.prependTo($mobileNav);

        // Insert the mobile links into mobile menu container.
        $mobileLinks.appendTo($mobileNav);

        // Add mobile menu to the page.
        $('.skiplinks', context).after($mobileNav);

        var $mobileMenuWrapper = $('.mobile-menu__links', context),
            $mobileMenuLinks = $mobileMenuWrapper.find('a');

        // Initially take mobile menu links out of tab flow.
        $mobileMenuLinks.attr('tabindex', -1);

        // Open/close mobile menu when menu button is clicked.
        $('.js-mobile-menu-button', context).click(function (e) {
          $(this).toggleClass('is-active');
          $mobileMenuWrapper.toggleClass('hidden');

          // Close search bar if open.
          if ($('.js-mobile-search-button').hasClass('is-active')) {
            $('.js-mobile-search-button').removeClass('is-active');
            $('.mobile-menu .mobile-menu__search').hide();
          }

          // Remove focus for mouse clicks after closing the menu.
          $(this).not('.is-active').mouseleave(function () {
            $(this).blur();
          });

          // Take mobile menu links out of tab flow if hidden.
          if ($mobileMenuWrapper.hasClass('hidden')) {
            $mobileMenuLinks.attr('tabindex', -1);
          }
          else {
            $mobileMenuLinks.removeAttr('tabindex');
          }

          e.preventDefault();
        });

        // Open/close submenus.
        $('.menu__subnav-arrow', context).click(function (e) {
          $(this).toggleClass('is-active').parent().toggleClass('is-open');
          $(this).siblings('.menu__subnav').slideToggle();

          // Remove focus for mouse clicks after closing the menu.
          $(this).not('.is-active').mouseleave(function () {
            $(this).blur();
          });

          e.preventDefault();
        });

        // Open/close search bar.
        $('.js-mobile-search-button', context).click(function (e) {
          $(this).toggleClass('is-active');

          // Close menu if open.
          if ($('.js-mobile-menu-button').hasClass('is-active')) {
            $('.js-mobile-menu-button').removeClass('is-active');
            $mobileMenuWrapper.addClass('hidden');
            $mobileMenuLinks.attr('tabindex', -1);
          }

          // Remove focus for mouse clicks after closing the menu.
          $(this).not('.is-active').mouseleave(function () {
            $(this).blur();
          });

          // Slide search bar.
          $('.mobile-menu .mobile-menu__search').slideToggle(200);

          e.preventDefault();
        });

        // Set the height of the menu.
        $mobileMenuWrapper.height($(document).height());
      }
    }
  };
})(jQuery, Drupal);
;
(function ($, Drupal) {

  'use strict';

  // Utility Menu
  Drupal.behaviors.utilityMenu = {
    attach: function (context) {

      const accountLink = $('.menu--utility .menu__link--account, .menu--utility-es .menu__link--account');
      const accountMenu = $('.menu--utility .menu__subnav, .menu--utility-es .menu__subnav');
      const langLink = $('.links li');

      // Function for opening account menu
      const openAccountMenu = function() {
        accountLink.addClass('is-open');
        accountMenu.addClass('is-open');

        setTimeout( function () {
          // If user clicks outside the account menu, close it
          $(document).one('click', function clickAway(e) {
            if (e.target.closest('.menu--utility .menu__subnav')) {
              $(document).one('click', clickAway);
            } else {
              closeAccountMenu();
            }
          });

          // Close account menu if user presses escape key
          $(document).one('keyup', function (e) {
            if (e.key === "Escape") {
              closeAccountMenu();

              // Unbind the document.one('click') set above
              $(document).unbind('click');
            }
          });
        },200)
      }

      // Function for closing account menu
      const closeAccountMenu = function() {
        accountLink.removeClass('is-open');
        accountMenu.removeClass('is-open');
      }

      // On account link click, toggle the account menu
      accountLink.once('account-link-processed').each(function () {
        $(this).on('click', function (e) {

          if ($('body').hasClass('user-logged-in')) {
            e.preventDefault();

            if ($(this).hasClass('is-open')) {
              console.log('has class is-open');
              closeAccountMenu();
            }
            else {
              console.log('does not have class is-open');
              openAccountMenu();
            }
          }
          
        });
      });

      // put the language switcher link inside our utility menu
      langLink.addClass('menu__link--language menu__link');
    }
  }

})(jQuery, Drupal);;
(function ($, Drupal) {

  'use strict';

  // Utility Menu
  Drupal.behaviors.modal = {
    attach: function (context) {

      const slidePlayButton = $('.slide .js-video-slide-play');
      const modalCloseButton = $('.js-close-modal');

      // Open Slide Video Modal on Trigger Click
      slidePlayButton.once().on('click', function (e) {
        e.preventDefault();
        const modal = $(this).parents('.js-slider').find('.modal');
        const video = $(this).parents('.slide').find('.js-video-slide-video > *').first().clone();
        modal.find('.modal__video').append(video);
        modal.addClass('is-active');

        setTimeout(function () {
          // Close modal if user clicks outside the video container
          $(document).one('click', function clickAway(e) {
            if (e.target.closest('.modal__video')) {
              $(document).one('click', clickAway);
            } else {
              closeModals();
            }
          });

          // Close modal if user presses escape key
          $(document).one('keyup', function (e) {
            if (e.key === "Escape") {
              closeModals();
              // Unbind the document.one('click') set above
              $(document).unbind('click');
            }
          });
        }, 0);
      });

      // Function for closing modals
      const closeModals = function () {
        $('.modal__video').empty();
        $('.modal').removeClass('is-active');
      }

      // Close modal if user clicks the close button
      modalCloseButton.on('click', function (e) {
        e.preventDefault();
        closeModals();
      })
    }
  }

})(jQuery, Drupal);;
(function ($) {

  'use strict';

  // Utility Menu
  Drupal.behaviors.userDrawer = {
    attach: function (context) {

      const userDrawerTrigger = $('.js-user-drawer-trigger');
      const userDrawer = $('.user-drawer');
      const userDrawerHandle = $('.user-drawer__handle');
      
      // Set current language
      if ($('body').hasClass('language-en')) {
        var language = 'english';
      }
      else {
        var language = 'spanish';
        userDrawerHandle.addClass('spanish')
        userDrawerHandle.find('a').text('Ver ms');
      }

      // If language is spanish, update tab text

      // Function for closing user drawer
      const openUserDrawer = function() {
        userDrawerTrigger.addClass('is-active');
        userDrawer.addClass('is-active');
        userDrawerHandle.addClass('is-active');

        if(language == 'english') {
          userDrawerHandle.find('a').text('HIDE');
        }
        else {
          userDrawerHandle.find('a').text('Ver menos');
        }

        // Close mobile menu if open
        if ($('.js-mobile-menu-button').hasClass('is-active')) {
          $('.js-mobile-menu-button').removeClass('is-active');
          $('.mobile-menu__links').addClass('hidden');
        }
      }

      // Function for opening user drawer
      const closeUserDrawer = function() {
        userDrawerTrigger.removeClass('is-active');
        userDrawer.removeClass('is-active');
        userDrawerHandle.removeClass('is-active');

        if (language == 'english') {
          userDrawerHandle.find('a').text('SHOW');
        }
        else {
          userDrawerHandle.find('a').text('Ver ms');
        }
      }

      // User Drawer Trigger in the header
      userDrawerTrigger.once().on('click', function (e) {
        e.preventDefault();
        if ($(this).hasClass('is-active')) {
          closeUserDrawer();
        }
        else {
          openUserDrawer();
        }
      });

      // User Drawer Handle
      userDrawerHandle.find('a').once().on('click', function (e) {
        e.preventDefault();
        if ($(this).parents('.user-drawer__handle').hasClass('is-active')) {
          closeUserDrawer();
        }
        else {
          openUserDrawer();
        }
      });

      // Close the user drawer when mobile menu is activated
      $('.js-mobile-menu-button').once().on('click', function() {
        closeUserDrawer();
      });

      // If drawer is open on page load, change the handle text
      if (userDrawer.hasClass('is-active')) {
        userDrawerHandle.addClass('is-active');
        userDrawerTrigger.addClass('is-active');

        if (language == 'english') {
          userDrawerHandle.find('a').text('HIDE');
        }
        else {
          userDrawerHandle.find('a').text('Ver menos');
        }
      }

    }
  }

})(jQuery);;
(function ($) {

  'use strict';

  // User Create/Edit Form
  Drupal.behaviors.userFormAccordions = {
    attach: function (context) {

      const submitButton = $('.user-form #edit-submit');
      const termsOfUseCheckbox = $('#edit-field-user-terms-of-use-value');
      const continueButton = $('.button--continue');
      var accordionTrigger = null;

      // Expand the next Accordion when the continue button is clicked
      continueButton.on('click', function( event ){
        event.preventDefault();
        accordionTrigger = $($(this).attr('href'));
        if (accordionTrigger) {
          accordionTrigger.once().click();
        }
      });
      // Only show the submit button if the terms of use is checked
      if (!termsOfUseCheckbox.attr('checked')){
        submitButton.hide();
      }

      termsOfUseCheckbox.on('change', function( event){
        if (0 < $(this).filter(':checked').length) {
          submitButton.show();
        }
        else {
          submitButton.hide();
        }
      });
    }
  }
})(jQuery);;
(function ($, Drupal) {

  'use strict';

  // Sliders
  Drupal.behaviors.sliders = {
    attach: function (context) {

      // Stacked Slider Implementation
      const stackedSlider = $('.stacked-slider__items');
      const scrollSlider = $('.scroll-slider__items');
      const carouselSlider = $('.carousel-slider__items');
      const featuredSlider = $('.featured-slider__items');

      stackedSlider.not('.slick-initialized').slick({
        dots: false,
        infinite: true,
        arrows: false,
        speed: 0,
        slidesToShow: 1,
        slidesToScroll: 1,
      });

      scrollSlider.not('.slick-initialized').slick({
        dots: false,
        infinite: true,
        arrows: false,
        speed: 0,
        slidesToShow: 1,
        slidesToScroll: 1,
      });

      carouselSlider.not('.slick-initialized').slick({
        dots: false,
        infinite: true,
        arrows: true,
        speed: 300,
        slidesToShow: 5,
        slidesToScroll: 1,
        responsive: [
          {
            breakpoint: 1100,
            settings: {
              slidesToShow: 4
            }
          },
          {
            breakpoint: 900,
            settings: {
              slidesToShow: 3
            }
          },
          {
            breakpoint: 650,
            settings: {
              slidesToShow: 2
            }
          },
          {
            breakpoint: 450,
            settings: {
              slidesToShow: 1
            }
          }
        ]
      });

      featuredSlider.not('.slick-initialized').slick({
        dots: false,
        infinite: true,
        arrows: true,
        speed: 300,
        slidesToShow: 3,
        slidesToScroll: 1,
        responsive: [
          {
            breakpoint: 1100,
            settings: {
              slidesToShow: 2
            }
          },
          {
            breakpoint: 550,
            settings: {
              slidesToShow: 1
            }
          }
        ]
      });

      // Advance the slider when user clicks arrows
      $('.slide__arrow--next').on('click', function (e) {
        e.preventDefault();
        $(this).parents('.slick-slider').slick('slickNext');
      });

      $('.slide__arrow--prev').on('click', function (e) {
        e.preventDefault();
        $(this).parents('.slick-slider').slick('slickPrev');
      });

      $('.slide__image-arrow--next').on('click', function (e) {
        e.preventDefault();
        $(this).parents('.slick-slider').slick('slickNext');
      });

      $('.slide__image-arrow--prev').on('click', function (e) {
        e.preventDefault();
        $(this).parents('.slick-slider').slick('slickPrev');
      });


      // Slide Counter Numbers
      $('.stacked-slider').each(function (index) {
        const slidesArray = $(this).find('.slide:not(.slick-cloned)');
        const counterTotal = slidesArray.length;
        slidesArray.each(function (item) {
          $(this).find('.slide__current').text((item + 1));
          $(this).find('.slide__total').text(counterTotal);
        });
      });

      $('.scroll-slider').each(function (index) {
        const slidesArray = $(this).find('.slide:not(.slick-cloned)');
        const counterTotal = slidesArray.length;
        slidesArray.each(function (item) {
          $(this).find('.slide__current').text((item + 1));
          $(this).find('.slide__total').text(counterTotal);
        });
      });

      const carouselCounter = function () {

        $('.carousel-slider').each(function (index) {
          const counterTotal = $(this).find('.view__row:not(.slick-cloned)').length;
          $(this).find('.carousel-slider__total').text(counterTotal);

          if (Modernizr.mq("screen and (min-width:1100px)")) {
            if (counterTotal > 5) {
              $(this).find('.carousel-slider__count').text(5);
            }
            else {
              $(this).find('.carousel-slider__count').text(counterTotal);
            }
          }
          else if (Modernizr.mq("screen and (min-width:900px)")) {
            if (counterTotal > 4) {
              $(this).find('.carousel-slider__count').text(4);
            }
            else {
              $(this).find('.carousel-slider__count').text(counterTotal);
            }
          }
          else if (Modernizr.mq("screen and (min-width:650px)")) {
            if (counterTotal > 3) {
              $(this).find('.carousel-slider__count').text(3);
            }
            else {
              $(this).find('.carousel-slider__count').text(counterTotal);
            }
          }
          else if (Modernizr.mq("screen and (min-width:450px)")) {
            if (counterTotal > 2) {
              $(this).find('.carousel-slider__count').text(2);
            }
            else {
              $(this).find('.carousel-slider__count').text(counterTotal);
            }
          }
          else {
            $('.carousel-slider__count').text(1);
          }

        });
      };


      // Arrow height on Carousel Slider
      const carouselArrowHeight = function () {
        const carouselArrow = $('.carousel-slider .slick-arrow');
        const carouselImageHeight = $('.carousel-slider .carousel-card__image').height();
        carouselArrow.css('height', carouselImageHeight);
      }
      carouselArrowHeight();


      // Arrow height on Featured Slider
      const featuredArrowHeight = function () {
        const featuredArrow = $('.featured-slider .slick-arrow');
        const featuredImageHeight = $('.featured-slider .teaser__image').outerHeight();
        featuredArrow.css('height', featuredImageHeight);
      }
      featuredArrowHeight();


      // Featured slider links
      $('.featured-slider__link--staff-picks').once().on('click', function (e) {
        e.preventDefault();
        $(this).parents('.featured-slider').find('.featured-slider__items').slick('slickGoTo', 0);
      })

      $('.featured-slider__link--newest').once().on('click', function (e) {
        e.preventDefault();
        $(this).parents('.featured-slider').find('.featured-slider__items').slick('slickGoTo', 6);
      })

      $('.featured-slider__link--popular').once().on('click', function (e) {
        e.preventDefault();
        $(this).parents('.featured-slider').find('.featured-slider__items').slick('slickGoTo', 12);
      })

      // Featured Slider After
      featuredSlider.on('afterChange', function () {
        const currentSlide = $(this).slick('slickCurrentSlide');

        if (currentSlide >= 0 && currentSlide < 6) {
          $('.featured-slider__link--newest').removeClass('is-active');
          $('.featured-slider__link--popular').removeClass('is-active');
          $('.featured-slider__link--staff-picks').addClass('is-active');
        }
        else if (currentSlide >= 6 && currentSlide < 12) {
          $('.featured-slider__link--staff-picks').removeClass('is-active');
          $('.featured-slider__link--popular').removeClass('is-active');
          $('.featured-slider__link--newest').addClass('is-active');
        }
        else if (currentSlide >= 12 && currentSlide < 18) {
          $('.featured-slider__link--newest').removeClass('is-active');
          $('.featured-slider__link--staff-picks').removeClass('is-active');
          $('.featured-slider__link--popular').addClass('is-active');
        }
      });

      // Featured slider arrow animation
      var scrollController = new ScrollMagic.Controller();

      var scrollScene = new ScrollMagic.Scene({
        duration: '100%', // resposive duration in %
        offset: 100, // offset trigger position by 100px
        triggerElement: '.featured-slider', // what will trigger scene
        triggerHook: 0.5, // 0=top, 0.5=middle, 1=bottom
        reverse: true // plays scene on the way up?
      }).addTo(scrollController);

      scrollScene.setClassToggle('.js-featured-explore', 'is-active');




      // Generic function that runs on window resize.
      function resizeStuff() {
        carouselArrowHeight();
        featuredArrowHeight();
        carouselCounter();
      }

      // Runs function once on window resize.
      var TO = false;
      $(window).resize(function () {
        if (TO !== false) {
          clearTimeout(TO);
        }

        // 200 is time in miliseconds.
        TO = setTimeout(resizeStuff, 200);
      }).resize();

    }
  }




})(jQuery, Drupal);
;
(function ($) {

  'use strict';

  // complex Teaser
  Drupal.behaviors.complexTeaser = {
    attach: function (context) {

      const complexTeaser = $('.teaser--complex');
      const teaserNoteLink = $('.teaser--complex .teaser__note-link');
      const teaserNotes = $('.teaser--complex .teaser__note');

      // Activate teaser note on link click
      teaserNoteLink.on('click', function(e) {
        e.preventDefault();
        $(this).parents('.teaser--complex').find('.teaser__note-link').removeClass('is-active');
        $(this).addClass('is-active');

        $(this).parents('.teaser--complex').find('.teaser__note').removeClass('note-active');
        if ($(this).hasClass('teaser__note-link--public')) {
          $(this).parents('.teaser--complex').find('.teaser__note--public').addClass('note-active');
        }
        else {
          $(this).parents('.teaser--complex').find('.teaser__note--private').addClass('note-active');
        }
      });
    }
  }

})(jQuery);;
(function ($, Drupal) {

  'use strict';

  // Utility Menu
  Drupal.behaviors.accordionTeaser = {
    attach: function (context) {


      // Add show more / hide link if content is tall enough
      const addAccordionShowHide = function() {
        const accordionTeaser = $('.accordion-teaser');
        
        accordionTeaser.each( function() {
          const contentHeight = $(this).find('.accordion-teaser__content').height();
          const imgHeight = $(this).find('.accordion-teaser__left').height();
          
          // If accordion teaser image exists
          if ($(this).find('.accordion-teaser__left').length ) {
            // If content is taller than the image, add class has-accordion
            contentHeight > imgHeight ? $(this).addClass('has-accordion') : $(this).removeClass('has-accordion');
          }
          else { // If there is no image, cut the content at 250 height
            contentHeight > 250 ? $(this).addClass('has-accordion') : $(this).removeClass('has-accordion');
          }
        });
      }
      addAccordionShowHide();


      // Show more on Accordion Teasers
      $('.accordion-teaser__show-more', context).once().on('click', function (e) {
        e.preventDefault();

        if ($(this).parents('.accordion-teaser').hasClass('is-expanded')) {
          $(this).parents('.accordion-teaser').removeClass('is-expanded');
          $(this).text('Show More');
        }
        else {
          $(this).parents('.accordion-teaser').addClass('is-expanded');
          $(this).text('Show Less');
        }
      });



      // Generic function that runs on window resize.
      function resizeStuff() {
        addAccordionShowHide();
      }

      // Runs function once on window resize.
      var TO = false;
      $(window).resize(function () {
        if (TO !== false) {
          clearTimeout(TO);
        }

        // 200 is time in miliseconds.
        TO = setTimeout(resizeStuff, 200);
      }).resize();

    }
  }

})(jQuery, Drupal);;
